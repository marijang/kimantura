  transition: transform 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 1.2s, opacity 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 1.2s;
  transition: transform 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 1.2s, opacity 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 1.2s, -webkit-transform 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 1.2s;
  margin-left: -7.5rem;
}

